{
  "expo": {
    "name": "Intelli Protect",
    "slug": "intelli-protect",
    "description": "This is the control app for intelli protect system.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deonachuo.intelliprotect"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.deonachuo.intelliprotect"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
